{
  "name": "@sanity/hierarchical-document-list",
  "version": "1.1.0",
  "description": "Sanity Plugin - Hierarchical Document List",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/sanity-io/hierarchical-document-list#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/hierarchical-document-list/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/hierarchical-document-list.git"
  },
  "license": "MIT",
  "author": "Sanity <hello@sanity.io>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js",
    "package.json"
  ],
  "scripts": {
    "prepare": "npm run build && husky install",
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "prepublishOnly": "run-s build",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^18.1.0",
    "@nosferatu500/react-sortable-tree": "^4.4.0",
    "@sanity/client": "^3.4.1",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/mutator": "^3.5.0",
    "@sanity/ui": "^1",
    "@sanity/util": "^3.19.1",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^1.17.2",
    "@sanity/plugin-kit": "^3.1.7",
    "@types/react": "^18.0.28",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prettier-plugin-packagejson": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^4.1.2",
    "sanity": "^3.19.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "tsc": false
    }
  }
